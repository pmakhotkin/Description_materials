<Page x:Class="Техописание_запчастей.View.NokPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Техописание_запчастей.View"
      xmlns:vm="clr-namespace:Техописание_запчастей.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NokPage"
      Background="White">
    <Page.DataContext>
        <vm:WelcomePageViewModel></vm:WelcomePageViewModel>
    </Page.DataContext>
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10">
            <TextBlock Text="Запчасти с неполными данными" />
            <DataGrid ItemsSource="{Binding NotValidSpareParts}" Margin="10"></DataGrid>
            <TextBlock Text="Запчасти которых нет в базе" />
            <ListBox ItemsSource="{Binding NotExistDBParts}" Margin="10"></ListBox>
            <TextBlock Text="Запчасти с неверными ссылками на фото" />
            <DataGrid ItemsSource="{Binding NotValidSpareParts}" Margin="10"></DataGrid>
        </StackPanel>
    </Grid>
</Page>
